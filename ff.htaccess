RewriteEngine on
 RewriteCond %{REQUEST_URI} ! \.[a-z]*$ [NC]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule (.*) $1.html [L]
 RewriteCond %{REQUEST_URI} ! \.[a-z]*$ [NC]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME}.jpg -f
RewriteRule (.*) $1.jpg [L]